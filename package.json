  },
  "dependencies": {
    "next": "12.0.7",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-syntax-highlighter": "^15.5.0"
  },
  "devDependencies": {
    "@types/node": "17.0.4",
    "@types/react": "17.0.38",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.10.0",
    "eslint-plugin-prettier": "^4.0.0",
    "lint-staged": "^12.3.5",
    "next-sitemap": "^2.5.10",
    "postcss": "^8.4.5",
    "prettier": "^2.6.0",
    "tailwindcss": "^3.0.7",
    "typescript": "4.6.2"
  },
  "lint-staged": {
    "*.+(js|jsx|ts|tsx)": [
